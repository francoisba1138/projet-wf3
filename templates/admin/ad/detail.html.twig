{% extends 'base.html.twig' %}
{% block title %}Annonce{% endblock %}

{% block body %}


    {{ form_start(form) }}
    {{ form_row(form.date) }}
    {{ form_row(form.title) }}
    {{ form_label(form.seller, 'Vendeur') }}
