{% extends 'base.html.twig' %}


{% block body %}


    {{ form_start(form) }}
    {{ form_row(form.firstname) }}
    {{ form_row(form.lastname) }}
    {{ form_row(form.shopName) }}
    {{ form_row(form.email) }}
    {{ form_row(form.address) }}
    {{ form_row(form.presentation) }}
    {%  if original_image %}
        <img src="{{ asset('img/profile/' ~ original_image) }}" height="200px">
    {% endif %}
    {{  form_row(form.image) }}


    {{ form_row(form.role) }}

    <input type="submit" value="Modifier" class="btn btn-outline-primary mb-3">
    {{ form_end(form) }}




{% endblock %}