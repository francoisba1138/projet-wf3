{% extends 'base.html.twig' %}
{% block title %}Ajout annonce{% endblock %}
{% block body %}
    <h1>Ajouter une annonce</h1>
    {{ form_start(form) }}
    {{ form_row(form.title) }}
    {{ form_row(form.game) }}
    {{ form_row(form.cond) }}
    {{ form_row(form.status) }}
    {{ form_row(form.content) }}
    {{ form_row(form.price) }}
    <input type="submit" value="Ajouter" class="btn btn-outline-primary mb-3">
    {{ form_end(form) }}
{% endblock %}

