{% extends 'base.html.twig' %}

{% block body %}
    {{ dump(response) }}

    {% for game in response %}
        {{ game.name }} - {{ game.slug }}<br>

        {% for platform in game.platforms %}
            {{ platform}}<br>
        {% endfor %}

        {% for release_date in game.release_dates %}
            {{ release_date }}<br>
        {% endfor %}

    {% endfor %}

{% endblock %}
